{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "ibrahimtestdatafactory"
		},
		"AzureDataExplorerLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'servicePrincipalKey' de 'AzureDataExplorerLinkedService'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'AzureStorageLinkedService'"
		},
		"AzureDataExplorerLinkedService_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://ibrahimtest.brazilsouth.kusto.windows.net"
		},
		"AzureDataExplorerLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "7e2215f3-dae3-4ac3-8e3b-88daa8db7d37"
		},
		"AzureDataExplorerLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "da538514-b5d8-4248-a658-65bbd3bee750"
		},
		"AzureDataExplorerLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "catalogsdb"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ibrahimtestdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataExplorerLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureDataExplorerLinkedService_servicePrincipalKey')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"AzureDataExplorerLinkedService_properties_typeProperties_endpoint": {
						"value": "[parameters('AzureDataExplorerLinkedService_properties_typeProperties_endpoint')]"
					},
					"AzureDataExplorerLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataExplorerLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureDataExplorerLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataExplorerLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataExplorerLinkedService_properties_typeProperties_database": {
						"value": "[parameters('AzureDataExplorerLinkedService_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}